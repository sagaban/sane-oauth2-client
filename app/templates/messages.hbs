<h1>Messages</h1>

<table class="table table-bordered table-striped">
    <thead>
      <tr>
          <th>Text</th>
          <th>Owner email</th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |message|}}
        <tr>
            <td>{{message.text}}</td>
            <td>{{message.owner.email}}</td>
        </tr>
    {{/each}}

    </tbody>
</table>
