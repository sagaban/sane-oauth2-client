<h1>Users</h1>

<table class="table table-bordered table-striped">
    <thead>
      <tr>
          <th>ID</th>
          <th>First Name</th>
          <th>E-mail</th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |user|}}
        {{#if user.email}}
          <tr>
              <td>{{user.id}}</td>
              <td>{{user.firstName}}</td>
              <td>{{user.email}}</td>
          </tr>
        {{/if}}
    {{/each}}

    </tbody>
</table>
