<h1>Users</h1>

<table class="table table-bordered table-striped">
    <thead>
      <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>E-mail</th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |user|}}
        <tr>
            <td>{{user.firstName}}</td>
            <td>{{user.lastName}}</td>
            <td>{{user.email}}</td>
        </tr>
    {{/each}}

    </tbody>
</table>
